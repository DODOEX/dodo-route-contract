 {
   "extends": "solhint:recommended",
   "plugins": [],
   "rules": {
     "compiler-version": ["error","0.8.16"],
     "func-visibility": ["warn", { "ignoreConstructors": true }],
     "avoid-suicide": "error",
     "avoid-sha3": "warn",
     "var-name-mixedcase": "ignore",
     "not-rely-on-time": "ignore"
   }
 }
